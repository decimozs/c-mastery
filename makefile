pointers:
	gcc -o pointers pointers.c
	./pointers

memoryAddress:
	gcc -o memoryAddress memoryAddress.c
	./memoryAddress

dereference:
	gcc -o dereference dereference.c
	./dereference

datatypes:
	gcc -o dataTypes dataTypes.c
	./dataTypes

arrayp:
	gcc -o arrayPointers arrayPointers.c
	./arrayPointers

arrays:
	gcc -o arrays arrays.c
	./arrays

default:
	gcc -o test test.c
	./test